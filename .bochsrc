# You may now use double quotes around pathnames, in case
# your pathname includes spaces.

#=======================================================================
# CONFIG_INTERFACE
#=======================================================================
#config_interface: textconfig

#=======================================================================
# DISPLAY_LIBRARY
# Using 'win32' as confirmed working display library.
#=======================================================================
display_library: win32 
#display_library: term  # Keep this as a text-only fallback option

#=======================================================================
# ROMIMAGE:
#=======================================================================
romimage: file=./BIOS-bochs-latest_2-3

#=======================================================================
# CPU:
# Setting a high IPS for better performance.
#=======================================================================
cpu: count=1, ips=100000000 

#=======================================================================
# MEGS
#=======================================================================
megs: 256

#=======================================================================
# OPTROMIMAGE[1-4]:
#=======================================================================
#optromimage1: file=optionalrom.bin, address=0xd0000

#optramimage1: file=/path/file1.img, address=0x0010000

#=======================================================================
# VGAROMIMAGE
#=======================================================================
vgaromimage: file=./VGABIOS-lgpl-latest

#=======================================================================
# VGA:
#=======================================================================
vga: extension=none

#=======================================================================
# FLOPPYA:
#=======================================================================
#floppya: 1_44=/dev/fd0, status=inserted

#=======================================================================
# FLOPPYB:
#=======================================================================
#floppyb: 1_44=b:, status=inserted

#=======================================================================
# ATA0, ATA1, ATA2, ATA3
#=======================================================================
ata0: enabled=1, ioaddr1=0x1f0, ioaddr2=0x3f0, irq=14

#=======================================================================
# ATA[0-3]-MASTER, ATA[0-3]-SLAVE
#=======================================================================
ata0-master: type=disk, mode=flat, path="./obj/kern/bochs.img"

#=======================================================================
# BOOT:
#=======================================================================
boot: disk

#=======================================================================
# CLOCK:
# Setting sync=none for better performance.
#=======================================================================
clock: sync=none, time0=local


#=======================================================================
# FLOPPY_BOOTSIG_CHECK: disabled=[0|1]
#=======================================================================
floppy_bootsig_check: disabled=0

#=======================================================================
# LOG:
#=======================================================================
log: bochs.log

#=======================================================================
# LOGPREFIX:
#=======================================================================
#logprefix: %t%e%d

#=======================================================================
# LOG CONTROLS
#=======================================================================
panic: action=ask
error: action=report
info: action=ignore
debug: action=ignore

#=======================================================================
# DEBUGGER_LOG:
#=======================================================================
debugger_log: -

#=======================================================================
# COM1, COM2, COM3, COM4:
#=======================================================================
#com1: enabled=1, mode=term, dev=/dev/ttyp9


#=======================================================================
# PARPORT1, PARPORT2:
# Changed path to "CON" for Windows console redirection.
# NOTE: Your FOS code MUST write to the parallel port (0x378) for this to work.
#=======================================================================
parport1: enabled=1, file="CON"
# parport1: enabled=1, file="-"  # Alternative for console output

#=======================================================================
# SB16:
#=======================================================================
#sb16: midimode=1, midi=/dev/midi00, wavemode=1, wave=/dev/dsp, loglevel=2, log=sb16.log, dmatimer=600000

#=======================================================================
# VGA_UPDATE_INTERVAL:
# Commented out as it was not understood by your Bochs version.
#=======================================================================
#vga_update_interval: 10000

#=======================================================================
# KEYBOARD_SERIAL_DELAY:
#=======================================================================
# keyboard_serial_delay: 250

#=======================================================================
# KEYBOARD_PASTE_DELAY:
#=======================================================================
# keyboard_paste_delay: 100000

#=======================================================================
# MOUSE: 
#=======================================================================
mouse: enabled=0

#=======================================================================
# private_colormap: 
#=======================================================================
private_colormap: enabled=0

#=======================================================================
# fullscreen: ONLY IMPLEMENTED ON AMIGA
#=======================================================================
#fullscreen: enabled=0

#=======================================================================
# ne2k: NE2000 compatible ethernet adapter
#=======================================================================
# ne2k: ioaddr=0x240, irq=9, mac=fe:fd:00:00:00:01, ethmod=fbsd, ethdev=en0 #macosx

#=======================================================================
# KEYBOARD_MAPPING:
#=======================================================================
# keyboard_mapping: enabled=0, map=

#=======================================================================
# KEYBOARD_TYPE:
#=======================================================================
#keyboard_type: mf

#=======================================================================
# USER_SHORTCUT:
#=======================================================================
#user_shortcut: keys=ctrl-alt-del

#=======================================================================
# I440FXSUPPORT:
#=======================================================================
#i440fxsupport: enabled=1

#=======================================================================
# USB1:
#=======================================================================
#usb1: enabled=1

#=======================================================================
# CMOSIMAGE:
#=======================================================================
#cmosimage: file=cmos.img, rtc_init=time0

#=======================================================================
# other stuff
#=======================================================================
#magic_break: enabled=1

#-------------------------
# PCI host device mapping
#-------------------------
#pcidev: vendor=0x1234, device=0x5678

#=======================================================================
# GDBSTUB:
#=======================================================================
#gdbstub: enabled=0, port=1234